{
    "name": "tannhatcms/ophim-core",
    "description": "Ophim's core features",
    "type": "library",
    "authors": [
        {
            "name": "hacoidev",
            "email": "hacoi.dev@gmail.com"
        }
    ],
    "require": {
        "laravel/framework": "^6|^7|^8|^9|^10.0",
        "ckfinder/ckfinder-laravel-package": "v3.5.2.1",
        "tannhatcms/laravel-caching-model": "^1.0.0",
        "tannhatcms/crud": "^1.0.0",
        "tannhatcms/settings": "^1.0.0",
        "tannhatcms/permissionmanager": "^1.0.0",
        "spatie/laravel-sitemap": "^5.8",
        "artesaos/seotools": "^v0.22.1"
    },
    "replace": {
        "hacoidev/ophim-core":"*"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Ophim\\Core\\": "src/",
            "Ophim\\Core\\Database\\Factories\\": "database/factories/",
            "Ophim\\Core\\Database\\Seeders\\": "database/seeders/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Ophim\\Core\\OphimServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable"
}
